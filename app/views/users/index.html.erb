<div id="userindex" class="container">
  <div class="row">
  	<div class="col-xs-12">
			<table class="table table-hover">
			  <thead>
			    <tr>
			      <th>Username</th>
			      <th>Reputation</th>
			    </tr>
			  </thead>
			  <tbody>
			  	<% @users.each do |user| %>
				    <tr>
				      <td><%= user.username %></td>
				      <td><%= user.id %></td>
				    </tr>
				  <% end %>
			  </tbody>
			</table>
  	</div>
  </div>
</div>
