<div id="userindex" class="container">
  <div class="row">
    <div class="col-xs-12">

      <h1>Listing Users</h1>

      <ul class="list-group">
        <% @users.each do |user| %>
          <li class="list-group-item">
            <%= link_to user.username, "/users/#{user.id}" %>
          </li>
        <% end %>
      </ul>

    </div>
  </div>
</div>