<div id="results" class="container">
  <div class="row">
    <div class="col-xs-12">

      <h1>Search Results</h1>

      <ul class="list-group">
        <% @questions.each do |q| %>
          <li class="list-group-item">
            <%= link_to q.title, q %> - asked <%= q.created_at.strftime("%b %d '%y at %-H:%M") %> by <%= q.user.name %>
          </li>
        <% end %>
      </ul>

    </div>
  </div>
</div>